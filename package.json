{
	"name": "jsx2svg",
	"displayName": "jsx2svg",
	"description": "Converts jsx into svg",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:jsx2svg.convert"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "jsx2svg.convert",
				"title": "Convert to SVG"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == javascript",
					"command": "jsx2svg.convert",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.57.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"@babel/core": "^7.14.6",
		"@babel/plugin-transform-react-jsx": "^7.14.5",
		"@babel/preset-env": "^7.14.5",
		"@babel/preset-react": "^7.14.5",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
}
